<section class="section">
  <div class="container">
    <h2 class="title">
      <%= gettext "Edit account" %>
    </h2>

    <%= render "_form.html", Map.put(assigns, :action, account_path(@conn, :update, @account)) %>
  </div>
</section>

<footer class="footer">
  <div class="container">
    <p>
      <span><%= link gettext("Back"), to: account_path(@conn, :index) %></span>
    </p>
  </div>
</footer>
