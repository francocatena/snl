{
  "lib/snl_web/channels/*_channel.ex": {
    "type": "channel",
    "alternate": "test/channels/{}_channel_test.exs"
  },

  "lib/snl_web/controllers/*_controller.ex": {
    "type": "controller",
    "alternate": "test/controllers/{}_controller_test.exs"
  },

  "lib/snl_web/views/*_view.ex": {
    "type": "view",
    "alternate": "test/views/{}_view_test.exs"
  },

  "test/channels/*_channel_test.exs": {
    "type": "tchannel",
    "alternate": "lib/snl_web/channels/{}_channel.ex"
  },

  "test/controllers/*_controller_test.exs": {
    "type": "tcontroller",
    "alternate": "lib/snl_web/controllers/{}_controller.ex"
  },

  "test/views/*_view_test.exs": {
    "type": "tview",
    "alternate": "lib/snl_web/views/{}_view.ex"
  },

  "lib/snl_web/templates/*.eex": {
    "type": "template"
  },

  "lib/snl_web/router.ex": {
    "type": "router"
  },

  "assets/css/*": {
    "type": "stylesheet"
  },

  "assets/js/*": {
    "type": "javascript"
  },

  "config/*.exs": {
    "type": "config"
  },

  "lib/*": {
    "type": "lib"
  },

  "priv/repo/migrations/*.exs": {
    "type": "migration"
  }
}
