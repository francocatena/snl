{
  "lib/snl_web/channels/*_channel.ex": {
    "type": "channel",
    "alternate": "test/snl_web/channels/{}_channel_test.exs"
  },

  "lib/snl_web/controllers/*_controller.ex": {
    "type": "controller",
    "alternate": "test/snl_web/controllers/{}_controller_test.exs"
  },

  "lib/snl_web/views/*_view.ex": {
    "type": "view",
    "alternate": "test/snl_web/views/{}_view_test.exs"
  },

  "lib/snl_web/plugs/*_plug.ex": {
    "type": "plug",
    "alternate": "test/snl_web/plugs/{}_plug_test.exs"
  },

  "test/snl_web/channels/*_channel_test.exs": {
    "type": "tchannel",
    "alternate": "lib/snl_web/channels/{}_channel.ex"
  },

  "test/snl_web/controllers/*_controller_test.exs": {
    "type": "tcontroller",
    "alternate": "lib/snl_web/controllers/{}_controller.ex"
  },

  "test/snl_web/views/*_view_test.exs": {
    "type": "tview",
    "alternate": "lib/snl_web/views/{}_view.ex"
  },

  "test/snl_web/plugs/*_plug_test.exs": {
    "type": "tplug",
    "alternate": "lib/snl_web/plugs/{}_plug.ex"
  },

  "test/snl/*_test.exs": {
    "type": "tapp",
    "alternate": "lib/snl/{}.ex"
  },

  "test/support/*.ex": {
    "type": "tsupport"
  },

  "lib/snl_web/templates/*.eex": {
    "type": "template"
  },

  "lib/snl_web/router.ex": {
    "type": "router"
  },

  "assets/css/*": {
    "type": "stylesheet"
  },

  "assets/js/*": {
    "type": "javascript"
  },

  "config/*.exs": {
    "type": "environment"
  },

  "lib/snl.ex": {
    "type": "app"
  },

  "lib/snl/*.ex": {
    "type": "app",
    "alternate": "test/snl/{}_test.exs"
  },

  "lib/snl_web.ex": {
    "type": "web"
  },

  "lib/snl_web/*": {
    "type": "web"
  },

  "priv/repo/seeds.exs": {
    "type": "seed"
  },

  "priv/repo/migrations/*.exs": {
    "type": "migration"
  },

  "mix.exs": {
    "type": "mix"
  }
}
